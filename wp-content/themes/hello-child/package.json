{
  "name": "hello-child-theme",
  "version": "1.0.0",
  "description": "Child theme for Hello Elementor with Vite build system",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "lint:js": "eslint assets/js/**/*.{js,ts}",
    "lint:css": "stylelint 'assets/scss/**/*.scss'",
    "lint:php": "phpcs --standard=WordPress .",
    "format:js": "prettier --write 'assets/js/**/*.{js,ts}'",
    "format:css": "prettier --write 'assets/scss/**/*.scss'",
    "format:php": "phpcbf --standard=WordPress .",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.0",
    "@types/wordpress__blocks": "^12.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "sass": "^1.66.0",
    "stylelint": "^15.10.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "typescript": "^5.1.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}
